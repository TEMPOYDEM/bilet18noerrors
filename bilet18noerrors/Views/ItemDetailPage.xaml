<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="bilet18noerrors.Views.ItemDetailPage"
             >

    <StackLayout Spacing="20" Padding="15">
        <StackLayout Orientation="Horizontal">
            <Button Text="На главную" Command="{Binding CancelCommand}" HorizontalOptions="FillAndExpand"></Button>
        </StackLayout>
        <WebView x:Name="webView" Source="https://en.wikipedia.org/wiki/Color" VerticalOptions="FillAndExpand" />
    </StackLayout>
    
</ContentPage>